  "scripts": {
    "build-bump-version": "./node_modules/.bin/babel-node ./units/urb-base-tools/build-bump-version.js",
    "build-deployment": "npm run build-schema && npm run build-webpack && npm run build-server",
    "build-schema": "./node_modules/.bin/babel-node ./units/urb-base-tools/build-units.js && ./node_modules/.bin/babel-node ./units/urb-base-tools/build-schema.js && ./node_modules/.bin/relay-compiler --src ./units/ --extensions=js --schema configuration/urb-base-server/graphql/schema.graphql && ./node_modules/.bin/relay-compiler --src ./units/ --extensions=jsx --schema configuration/urb-base-server/graphql/schema.graphql",
    "build-server": "export NODE_ENV=production && rm -rf deployment/* && cp -r ./configuration/ ./deployment/configuration/ && ./node_modules/.bin/babel -s true --retain-lines -d ./deployment/configuration/ ./configuration && ./node_modules/.bin/babel -s true --retain-lines -d ./deployment/units/ ./units && cp ./units/urb-base-webapp/html.ejs ./deployment/units/urb-base-webapp/html.ejs &&cp ./package.json ./deployment && cp ./yarn.lock ./deployment",
    "build-units": "./node_modules/.bin/babel-node ./units/urb-base-tools/build-units.js",
    "build-webpack": "export NODE_ENV=production && rm -rf deployment/configuration/urb-base-server/public_files/assets/* &&  ./node_modules/.bin/webpack -p --output-filename client.js",
    "dev": "concurrently --kill-others \"npm run dev-relay\" \"npm run dev-webpack\" \"npm run dev-server\" ",
    "dev-reset": "sudo chmod -R a+w * && watchman watch-del-all && rm -rf ~/Library/Developer/Xcode/DerivedData/* && rm -rf $TMPDIR/react-* && rm -rf ~/.node-gyp && rm -f ./ios/main.* && rm -f ./android/app/src/main/assets/index.android.*",
    "dev-prod": "export NODE_ENV=production && ./node_modules/.bin/babel-node ./units/urb-base-server/server.js",
    "dev-server": "export NODE_ENV=development && ./node_modules/.bin/nodemon -e js,jsx --exec ./node_modules/.bin/babel-node -- ./units/urb-base-server/server.js",
    "dev-webpack": "./node_modules/.bin/webpack-dev-server --hot --inline --no-info --colors",
    "dev-relay": "./node_modules/.bin/relay-compiler --src ./units/ --extensions=js,jsx --schema configuration/urb-base-server/graphql/schema.graphql --watch",
    "setup-local": "cp ./configuration/default.env ./.env && cp ./configuration/app/default.publicURL.js ./configuration/app/publicURL.js && npm run update-ip && react-native link && (cd .git/hooks && ln -s -f ../../units/urb-base-tools/pre-commit.sh ./pre-commit)",
    "setup-local-cut-version": "cp ./configuration/default.env ./.env && cp ./configuration/app/default.publicURL.js ./configuration/app/publicURL.js",
    "setup-database": "./node_modules/.bin/babel-node ./units/urb-base-tools/setup-database.js",
    "update-default-persister": "./node_modules/.bin/babel-node ./units/urb-base-tools/update-default-persister.js",
    "update-ip": "./node_modules/.bin/babel-node ./units/urb-base-tools/update-ip.js"
  }
